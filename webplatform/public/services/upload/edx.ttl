    usdl4edu:hasLanguage usdl4edu:LanguageEN ;
    usdl4edu:hasOverallObjective usdl4edu:Age-of-Globalization-objectives ;
    usdl4edu:hasTeacher usdl4edu:John-Hoberman ;
    usdl4edu:hasTitle "Age of Globalization" .

